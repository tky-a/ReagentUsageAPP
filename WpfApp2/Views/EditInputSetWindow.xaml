<Window x:Class="WpfApp2.Views.EditInputSetWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfApp2.Views"
             mc:Ignorable="d" 
             Height="200" Width="250"
             >
    <Grid Background="White">
        <StackPanel>
            <TextBlock Text="ID" Margin="0 10 0 0"/>
            <TextBox Text="{Binding InputReagentId}"
                     Width="auto"
                     Background="WhiteSmoke"
                     Margin="10 0 0 0"/>
            <TextBlock Text="Name" Margin="0 10 0 0"/>
            <TextBox Text="{Binding InputReagentName}"
                     Width="auto"
                     Background="WhiteSmoke"
                     Margin="10 0 0 0"/>
            <Button Content="Save" Margin="5" Click="Button_Click"/>
        </StackPanel>
            
    </Grid>
</Window>
